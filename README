# How to create a docker image
sudo docker login gitlab-registry.cern.ch
sudo docker build . -f Dockerfile -t gitlab-registry.cern.ch/jbossios/docker-images/CUSTOM
sudo docker push gitlab-registry.cern.ch/jbossios/docker-images/CUSTOM

Repository:
https://gitlab.cern.ch/jbossios/docker-images

Used:
1. atlas-spanet-jona-dijets-test
2. atlas-spanet-jona-dijets-final
3. atlas-spanet-jona-dijets-set-num-workers
4. atlas-spanet-jona-dijets-set-num-workers-rm-old-output
5. atlas-spanet-jona-dijets-set-num-workers-rm-old-output-more-prints
6. atlas-spanet-jona-dijets-eval
